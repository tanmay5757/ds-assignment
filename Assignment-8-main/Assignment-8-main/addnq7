#include <bits/stdc++.h>
using namespace std;

struct Node{
    int data;
    Node* left;
    Node* right;
    Node(int x){
        data = x;
        left = right = NULL;
    }
};

void inorder(Node* root, vector<int> &v){
    if(!root) return;
    inorder(root->left, v);
    v.push_back(root->data);
    inorder(root->right, v);
}

struct DLL{
    int data;
    DLL* prev;
    DLL* next;
    DLL(int x){
        data = x;
        prev = next = NULL;
    }
};

DLL* makeDLL(vector<int> &v){
    if(v.empty()) return NULL;

    DLL* head = new DLL(v[0]);
    DLL* curr = head;

    for(int i=1;i<v.size();i++){
        DLL* temp = new DLL(v[i]);
        curr->next = temp;
        temp->prev = curr;
        curr = temp;
    }
    return head;
}

int main(){
    // BST 1
    Node* a = new Node(20);
    a->left = new Node(10);
    a->right = new Node(30);

    // BST 2
    Node* b = new Node(25);
    b->left = new Node(5);
    b->right = new Node(70);

    vector<int> v1, v2;
    inorder(a, v1);
    inorder(b, v2);

    vector<int> merged;
    int i = 0, j = 0;

    while(i < v1.size() && j < v2.size()){
        if(v1[i] < v2[j]) merged.push_back(v1[i++]);
        else merged.push_back(v2[j++]);
    }

    while(i < v1.size()) merged.push_back(v1[i++]);
    while(j < v2.size()) merged.push_back(v2[j++]);

    DLL* head = makeDLL(merged);

    // print dll
    DLL* t = head;
    while(t){
        cout << t->data << " ";
        t = t->next;
    }
}
